{
  "name": "theme",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build-css": "sass style.scss:../static/css/style.css --style=expanded",
    "watch-css": "sass --watch style.scss:static/css/style.css",
    "prefix-css": "postcss ../static/css/style.css --replace --use autoprefixer -b 'defaults'",
    "prod": "npm run build-css && npm run prefix-css",
    "dev": "npm run build-css && npm run prefix-css"
  },
